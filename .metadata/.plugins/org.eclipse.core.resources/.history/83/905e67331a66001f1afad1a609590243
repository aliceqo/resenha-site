function editarResenha(id) {
  const xhr = new XMLHttpRequest();
  xhr.open('GET', 'excluirEditar?id=' + id, true);
  xhr.send();
}

function excluirResenha(id) {
  const xhr = new XMLHttpRequest();
  xhr.open('DELETE', 'excluirEditar?id=' + id, true);
  xhr.send();
}

function atualizarResenha(id, formData) {
  const xhr = new XMLHttpRequest();
  xhr.open('POST', 'excluirEditar' + id, true);
  xhr.send(formData);
}